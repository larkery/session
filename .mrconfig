
[$HOME/.emacs.d]
checkout = git clone 'ssh://git@github.com/larkery/emacs.git' '.emacs.d'

[$HOME/.xmonad]
checkout = git clone 'ssh://git@github.com/larkery/xmonad.git' '.xmonad'

[$HOME/notes]
checkout = git clone 'ssh://lrkry.com/home/hinton/repos/notes' 'notes'

[$HOME/.password-store]
checkout = git clone 'ssh://lrkry.com:/home/hinton/repos/pass' '.password-store'

[$HOME/.config/vcsh/repo.d/session.git]
checkout = vcsh clone ssh://git@github.com/larkery/session.git session

[.config/i3]
checkout = git clone 'git@github.com:larkery/i3.git' 'i3'
