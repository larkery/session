source ~/.profile

redshift -l 51.455313:-2.591902 &
taffybar &
xsettingsd &
dunst &
xss-lock lock &
xsetroot -solid black -cursor_name left_ptr
setxkbmap &

echo ${DBUS_SESSION_BUS_ADDRESS} > .dbus_session_bus_address

export _JAVA_AWT_WM_NONREPARENTING=1

gpg-connect-agent -q updatestartuptty /bye
passm -s &

every 15m notmuch new > ~/.mail-log 2>&1 </dev/null &
reanimate 5s ~/.local/session/scripts/displays >/dev/null 2>&1 </dev/null &